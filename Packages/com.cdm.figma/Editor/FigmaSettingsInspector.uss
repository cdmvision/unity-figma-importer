.heading {
    margin: 5px 0px 10px;
    font-size: 20px;
    -unity-font-style: bold;
}
.panel-container{
    flex-direction: row;
    flex-wrap: wrap;
}

.panel {
    flex-direction: row;
    min-width: 500px;
    min-height: 400px;
}

.panel25{
    width:25%;
    flex-shrink:1;
}

.panel50{
    width:50%;
    flex-shrink:1;
}

.panel75{
    width:75%;
    flex-shrink:1;
}

.horizontalcontainer{
    flex-direction: row;
    flex-grow:1;
}
